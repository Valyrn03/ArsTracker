  <Label fx:id="characterAge"/>

  <HBox>
    <VBox>
      <HBox fx:id="basicStats" spacing="6.0" alignment="CENTER"></HBox>
    </VBox>

    <VBox>
      <HBox fx:id="abilityTable" spacing="6.0" alignment="CENTER"></HBox>
    </VBox>
  </HBox>
</VBox>
